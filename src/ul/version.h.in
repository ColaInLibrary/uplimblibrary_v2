#ifndef UL_VERSION_H
#define UL_VERSION_H

#define UL_VERSION UL_VERSION_MACRO(UL_VERSION_MAJOR, UL_VERSION_MINOR, UL_VERSION_PATCH)
#define UL_VERSION_MACRO(major, minor, patch) ((major << 16) | (minor << 8) | patch)
#define UL_VERSION_MAJOR @PROJECT_VERSION_MAJOR@
#define UL_VERSION_MINOR @PROJECT_VERSION_MINOR@
#define UL_VERSION_PATCH @PROJECT_VERSION_PATCH@
#define UL_VERSION_STRING "@PROJECT_VERSION@"

#endif // UL_VERSION_H
